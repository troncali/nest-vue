{
	"npmScope": "nest-vue",
	"affected": {
		"defaultBase": "main"
	},
	"workspaceLayout": {
		"appsDir": "apps",
		"libsDir": "libs"
	},
	"implicitDependencies": {
		"workspace.json": "*",
		"package.json": {
			"dependencies": "*",
			"devDependencies": "*"
		},
		"tsconfig.base.json": "*",
		".eslintrc.json": "*",
		"nx.json": "*"
	},
	"tasksRunnerOptions": {
		"default": {
			"runner": "nx/tasks-runners/default",
			"options": {
				"cacheableOperations": [
					"build",
					"lint",
					"merge",
					"test",
					"e2e"
				],
				"cacheDirectory": ".yarn/cache/nx"
			}
		}
	},
	"targetDependencies": {
		"build": [
			{
				"target": "build",
				"projects": "dependencies"
			}
		]
	},
	"cli": {
		"defaultCollection": "@nrwl/nest"
	},
	"defaultProject": "backend",
	"plugins": [
		"@nxrs/cargo"
	]
}
