{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"composite": true,
		"lib": [],
		"types": ["node", "jsdom"],
		"outDir": "../../.yarn/cache/out-tsc",
		"paths": {
			"@/*": ["./src/*"]
		}
	},
	"include": [
		"**/src/**/*.spec.ts",
		"**/src/**/*.test.ts",
		"**/src/**/*.spec.tsx",
		"**/src/**/*.test.tsx",
		"**/src/**/*.vue",
		"**/src/env.d.ts"
	],
	"exclude": []
}
