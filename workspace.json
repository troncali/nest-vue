			"targets": {
				"build": {
					"executor": "@nxrs/cargo:build",
					"options": {
						"release": false,
						"targetDir": "dist/apps/placeholder"
					},
					"configurations": {
						"production": {
							"release": true,
							"targetDir": "dist/apps/placeholder"
						}
					}
				},
				"test": {
					"executor": "@nxrs/cargo:test",
					"options": {}
				},
				"lint": {
					"executor": "@nxrs/cargo:clippy",
					"options": {
						"fix": false,
						"failOnWarnings": true,
						"noDeps": true
					}
				}
			},
			"tags": []
		}
