{
	"extends": "./tsconfig.json",
	"compilerOptions": {
		"outDir": "../../../.yarn/cache/out-tsc",
		"module": "commonjs",
		"types": ["jest", "node"]
	},
	"include": [
		"**/*.ts",
		"../../nest/**/*.ts",
		"**/*.test.ts",
		"**/*.spec.ts",
		"**/*.test.tsx",
		"**/*.spec.tsx",
		"**/*.test.js",
		"**/*.spec.js",
		"**/*.test.jsx",
		"**/*.spec.jsx",
		"**/*.d.ts",
		"jest.config.ts"
	]
}
